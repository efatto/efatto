<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <record id="view_account_journal_form_sequence" model="ir.ui.view">
        <field name="name">sequence.account.journal.form</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <field name="code" position="before">
                <field name="sequence"/>
            </field>
        </field>
    </record>

    <record id="view_account_journal_tree_sequence" model="ir.ui.view">
        <field name="name">sequence.account.journal.tree</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_tree"/>
        <field name="arch" type="xml">
            <field name="code" position="before">
                <field name="sequence" widget="handle"/>
            </field>
        </field>
    </record>


</data>
</openerp>

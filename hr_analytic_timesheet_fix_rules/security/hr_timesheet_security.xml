<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    	<delete id="hr_timesheet.property_rule_timesheet_employee" model="ir.rule"/>

    	<record model="ir.rule" id="hr_timesheet.property_rule_timesheet_employee">
            <field name="name">Employee HR Analytic Timesheet with read permission</field>
            <field name="model_id" ref="hr_timesheet.model_hr_analytic_timesheet"/>
            <field name="domain_force">[('user_id', '=', user.id)]</field>
            <field name="groups" eval="[(4,ref('base.group_user'))]"/>
            <field name="perm_read" eval="0"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>
    </data>
</openerp>

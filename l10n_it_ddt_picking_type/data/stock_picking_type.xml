<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_picking_type_customer_in" model="ir.sequence">
        <field name="name">Picking customer in</field>
        <field name="prefix">CU/IN/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="picking_type_customer_in" model="stock.picking.type">
        <field name="name">Customer In</field>
        <field name="sequence_id" ref="seq_picking_type_customer_in"/>
        <field name="default_location_src_id" ref="stock.stock_location_customers"/>
        <field name="default_location_dest_id" ref="stock.stock_location_stock"/>
        <field name="code">incoming</field>
    </record>
    <record id="seq_picking_type_supplier_out" model="ir.sequence">
        <field name="name">Picking supplier out</field>
        <field name="prefix">SU/OUT/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="picking_type_out_supplier" model="stock.picking.type">
        <field name="name">Supplier out</field>
        <field name="sequence_id" ref="seq_picking_type_supplier_out"/>
        <field name="default_location_src_id" ref="stock.stock_location_stock"/>
        <field name="default_location_dest_id" ref="stock.stock_location_suppliers"/>
        <field name="code">outgoing</field>
    </record>
</odoo>

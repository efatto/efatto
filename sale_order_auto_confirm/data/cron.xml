<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record forcecreate="True" id="ir_cron_confirm_sale_quotation_action" model="ir.cron">
        <field name="name">Confirm sent sale orders</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True" />
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="nextcall" eval="datetime.now().replace(hour=21, minute=0, second=0).strftime('%Y-%m-%d %H:%M:%S')" />
        <field name="code">model.confirm_sale_orders_in_waiting_confirm((3, [(1,1),(6,1),(25,4),(1,5),(2,6),(15,8),(1,11),(8,12),(25,12),(26,12),(31,12)]))</field>
        <field name="doall" eval="False"/>
    </record>
</odoo>
